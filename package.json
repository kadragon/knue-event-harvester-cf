    "@vitest/coverage-v8": "^3.2.4",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "typescript": "^5.4.5",
    "vite": "^7.3.0",
    "vitest": "^3.2.4",
    "wrangler": "^4.54.0"
